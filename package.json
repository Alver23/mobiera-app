{
  "name": "mobieraapp",
  "description": "Mobile app with typescript, Redux Toolkit, Hooks, api Context",
  "version": "1.1.0",
  "scripts": {
    "android": "react-native run-android && npm run reactotron:enable:android && adb reverse tcp:5001 tcp:5001",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "ci": "npm run lint && npm run test",
    "env:dev": "cat ./config/environments/.env ./config/environments/.env.development > .env",
    "env:prod": "cat ./config/environments/.env ./config/environments/.env.production > .env",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "jetifier",
    "release": "standard-version",
    "reset:cache": "npm run start -- --reset-cache",
    "reset:cache:android": "cd android && ./gradlew clean && npm run reset:cache",
    "reactotron:enable:android": "adb reverse tcp:9090 tcp:9090",
    "start": "react-native start",
    "sonarqube": "gulp sonarqube",
    "test": "jest --detectOpenHandles --logHeapUsage",
    "test:watch": "npm test -- --verbose --watch",
    "server": "node src/server/server.js",
    "server:generate": "json-server src/server/generate.js",
    "server:watch": "nodemon src/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alver23/mobiera-app.git"
  },
  "keywords": [
    "Redux",
    "Typescript",
    "React Native",
    "Redux Toolkit",
    "Custom Hooks",
    "Hooks",
    "Api Context",
    "JSON Server",
    "Faker Generator"
  ],
  "author": "Alver Grisales <viga.23@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alver23/mobiera-app/issues"
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "2.4.11",
    "@hookform/resolvers": "2.0.0",
    "@react-native-async-storage/async-storage": "1.14.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.9.3",
    "@react-navigation/stack": "5.14.3",
    "@reduxjs/toolkit": "1.5.1",
    "@testing-library/react-hooks": "5.1.1",
    "axios": "0.21.1",
    "class-transformer": "0.4.0",
    "es6-shim": "0.35.6",
    "react": "17.0.1",
    "react-hook-form": "7.0.0",
    "react-native": "0.72.0",
    "react-native-config": "1.4.2",
    "react-native-elements": "3.3.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-picker": "3.3.2",
    "react-native-reanimated": "2.1.0",
    "react-native-restart": "0.0.22",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.0.0",
    "react-native-sensitive-info": "5.5.8",
    "react-native-vector-icons": "8.1.0",
    "react-redux": "7.2.3",
    "redux": "4.0.5",
    "reflect-metadata": "0.1.13",
    "reselect": "4.0.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "7.12.5",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@types/react-native": "0.64.0",
    "@types/react-native-restart": "0.0.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "coveralls": "3.1.0",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.23.1",
    "faker": "5.5.2",
    "gulp": "4.0.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-html-reporters": "2.1.2",
    "jest-sonar-reporter": "2.0.0",
    "jetifier": "1.6.6",
    "json-server": "0.16.3",
    "lint-staged": "10.5.4",
    "lodash.times": "4.3.2",
    "metro-react-native-babel-preset": "0.64.0",
    "nodemon": "2.0.7",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "5.0.0",
    "reactotron-redux": "3.1.3",
    "sonarqube-scanner": "2.8.0",
    "standard-version": "9.1.1",
    "ts-node": "9.1.1",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "@types/react": "17"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
